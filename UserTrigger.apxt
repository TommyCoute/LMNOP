trigger UserTrigger on User (after update) {
    if (Trigger.isAfter && Trigger.isUpdate) {
        // update contact name to match user name changes
        // code coverage: lmnopTest
        MovieNightExtension.updateContactWithUser(Trigger.new);
    }
}